{
  "name": "openaichallenge",
  "version": "0.1.0",
  "homepage": "https://gpt3api.herokuapp.com/",
  "private": true,
  "dependencies": {
    "@babel/runtime": "^7.18.3",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^0.27.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "node-fetch": "^3.2.6",
    "nodemon": "^1.3.3",
    "openai": "^2.0.5",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "proxy": "http://localhost:8000",
  "scripts": {
    "start:backend": "node api.js",
    "start-client": "react-scripts start",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "nodemon api.js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "heroku-postbuild": "npm install && npm install --only=dev --no-shrinkwrap && npm run build",
    "dev": "concurrently --kill-others-on-fail \"npm start\" \"npm start-client\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^4.0.0"
  }
}
